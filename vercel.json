{
    "version": 2,
    "builds": [
     {
      "src": "app.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
     }
    ],
    "routes": [
     {
      "src": "/(.*)",
      "dest": "app.js"
     }
    ],
    "functions": {
        "app.js": {
          "maxDuration": 60
        },
        "api/*.js": {
          "maxDuration": 60
        }
    }
   }