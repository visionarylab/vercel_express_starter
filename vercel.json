{
    "version": 2,
    "routes": [
     {
      "src": "/(.*)",
      "dest": "app.js"
     }
    ],
    "functions": {
        "app.js": {
          "runtime":"@vercel/node", 
          "maxDuration": 60,
          "includeFiles": ["dist/**"]
        },
        "api/*.js": {
          "maxDuration": 60
        }
    }
}